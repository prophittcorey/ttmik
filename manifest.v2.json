{
  "name": "Talk to Me in Korean - Improved",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "A browser extension that makes subtle improvements to the TTMIK (talktomeinkorean.com) website.",
  "icons": {
    "16": "assets/images/icon.png",
    "48": "assets/images/icon.png",
    "128": "assets/images/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://talktomeinkorean.com/*"
      ],
      "all_frames": false,
      "js": [
        "js/content.js"
      ]
    }
  ]
}
